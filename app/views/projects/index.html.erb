<div class="header"> <h1>Projects</h1></div>
<div class="nav">
  <%= link_to "Add Project", new_project_path %>
</div>


<br>
<table>
  <thead>

  <tr>
    <th>Title</th>
    <th>Created</th>
    <th colspan="3"></th>
  </tr>

  </thead>

  <tbody>

  <% @projects.each do |project| %>
      <tr>
        <td><%= link_to project.name, project %></td>
        <td><%= project.created_at%></td>

      </tr>
  <% end %>

  </tbody>
</table>




